<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SpreadCreateView">
        <attr name="plus_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ClockView">
        <attr name="pointer_color" format="color" />
    </declare-styleable>

    <declare-styleable name="MonitorDetailView">
        <attr name="draw_color" format="color" />
        <attr name="circle_color" format="color" />
        <attr name="stroke_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PlanView">
        <attr name="plan_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CurrentPlanView">
        <attr name="current_plan_color" format="color" />
    </declare-styleable>

    <declare-styleable name="LabelView">
        <attr name="label_color" format="color" />
        <attr name="label_degree" format="integer" />
        <attr name="label_type" format="integer" />
    </declare-styleable>

    <declare-styleable name="VoucherView">
        <attr name="voucher_type">
            <enum name="free" value="0" />
            <enum name="free1" value="1" />
            <enum name="free2" value="2" />
            <enum name="free3" value="3" />
        </attr>
    </declare-styleable>


    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

</resources>
